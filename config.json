    "input_port": "MixTrack Platinum FX:MixTrack Platinum FX MIDI 1 32:0",
    "output_port": "MixTrack Platinum FX:MixTrack Platinum FX MIDI 1 32:0"
  },
  "display": {
    "bpm_display_type": 1,
    "time_display_type": 4
